{
  "name": "be-ts-db-lib",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/be-ts-db-lib/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/{projectRoot}",
        "currentVersionResolver": "git-tag",
        "fallbackCurrentVersionResolver": "disk"
      }
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/be-ts-db-lib",
        "main": "packages/be-ts-db-lib/src/index.ts",
        "tsConfig": "packages/be-ts-db-lib/tsconfig.build.json",
        "assets": ["packages/be-ts-db-lib/*.md"]
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    }
  }
}
