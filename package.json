{
  "name": "ts-libs",
  "private": true,
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@10.12.1",
  "scripts": {
    "build": "pnpm -r --stream build",
    "watch": "tsx scripts/watchAll.ts",
    "unpub": "pnpm -r --stream unpub:local",
    "lint": "pnpm -r --stream lint",
    "test": "pnpm -r --stream test",
    "preparePkg": "pnpm -r --stream --parallel preparePkg && pnpm -r --stream propagateVersion && pnpm i",
    "upgrade:core": "pnpm update '@aneuhold/*' --latest && pnpm -r --stream upgrade:core",
    "upgrade:all": "pnpm up --latest && pnpm -r --stream upgrade:all"
  },
  "devDependencies": {
    "@aneuhold/eslint-config": "^1.3.20",
    "@types/node": "^24.10.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.2",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  }
}
