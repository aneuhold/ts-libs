{
  "name": "@aneuhold/be-ts-lib",
  "author": "Anton G. Neuhold Jr.",
  "license": "MIT",
  "version": "2.0.55",
  "description": "A backend TypeScript library used for common functionality in personal backend projects.",
  "packageManager": "yarn@4.6.0",
  "type": "module",
  "scripts": {
    "build": "rimraf lib && tsc --project tsconfig.build.json && tsx ./scripts/copyTsFiles.ts",
    "lint": "eslint",
    "test": "vitest run",
    "jsr:validate": "tb pkg validateJsr",
    "checkAll": "yarn build && yarn lint && yarn test && yarn jsr:validate",
    "jsr:publish": "tb pkg publishJsr",
    "upgrade:core": "yarn up '@aneuhold/*'",
    "upgrade:all": "yarn up"
  },
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aneuhold/be-ts-lib.git"
  },
  "bugs": {
    "url": "https://github.com/aneuhold/be-ts-lib/issues"
  },
  "homepage": "https://github.com/aneuhold/be-ts-lib#readme",
  "keywords": [
    "Backend",
    "Node.js",
    "TypeScript"
  ],
  "dependencies": {
    "@aneuhold/core-ts-api-lib": "^2.0.13",
    "@aneuhold/core-ts-lib": "^2.0.10",
    "dotenv": "^16.4.7",
    "jsonc-parser": "^3.3.1",
    "node-fetch": "^2",
    "octokit": "^3.1.2"
  },
  "devDependencies": {
    "@aneuhold/eslint-config": "^1.0.83",
    "@aneuhold/main-scripts": "^2.0.13",
    "@types/node": "^22.14.0",
    "@types/node-fetch": "^2.6.12",
    "eslint": "^9.24.0",
    "jsr": "^0.13.4",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  }
}
